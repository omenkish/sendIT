{
  "name": "ui",
  "version": "1.0.0",
  "description": "A courier service app that delivers parcels to different destinations",
  "main": "index.js",
  "scripts": {
    "client": "webpack-dev-server --mode development",
    "test": "cross-env NODE_ENV=test nyc mocha server/test/**/*.js --require @babel/register --timeout 6000 --exit",
    "start": "node index.js",
    "heroku-prebuild": "npm install",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev-start": "cross-env NODE_ENV=development nodemon server --exec \"node -r @babel/register\"",
    "drop-tables": "nodemon server/helpers/dropTables --exec \"node -r @babel/register\"",
    "dev": "concurrently \"npm run dev-start\" \"npm run client\""
  },
  "nyc": {
    "exclude": [
      "UI",
      "server/test",
      "server/JSObject",
      "server/models",
      "server/client",
      "server/helpers"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omenkish/sendIT.git"
  },
  "author": "Eneojo Omede",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omenkish/sendIT/issues"
  },
  "homepage": "https://github.com/omenkish/sendIT#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "nodemailer": "^4.7.0",
    "pg": "^7.6.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "validatorjs": "^3.15.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": "10.x"
  }
}
