{
  "name": "ui",
  "version": "1.0.0",
  "description": "A courier service app that delivers parcels to different destinations",
  "main": "index.js",
  "scripts": {
    "tests": "mocha server/test/**/*.test.js --require @babel/register || true",
    "test": " cross-env NODE_ENV = test nyc mocha server/test/**/*.test.js --require @babel/register || true",
    "start": "node dist/server.js",
    "build": "npx babel server --out-dir dist ",
    "heroku-prebuild": "npm install",
    "coverage" : "nyc report --reporter=text-lcov || coveralls",
    "dev-start": "nodemon server/server --exec \"node -r @babel/register\""
  },

  "nyc": {
    "exclude": [
      "UI",
      "server/test"
    ],

    "require": [
      "@babel/register"
    ],

    "reporter": [
      "lcov",
      "text"
    ],

    "sourceMap": false,
    "instrument": false
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omenkish/sendIT.git"
  },
  "author": "Eneojo Omede",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omenkish/sendIT/issues"
  },
  "homepage": "https://github.com/omenkish/sendIT#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "uuid": "^3.3.2",
    "validatorjs": "^3.15.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": "10.x"
  }
}
